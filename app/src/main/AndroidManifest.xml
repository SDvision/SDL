<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="audioapk.com.example.android.farmertofarmer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--Login-->
        <activity
            android:name=".LogIn"
            android:theme="@style/RemoveActionBar">

        </activity>

        <!--Profile-->
        <activity android:name=".Profile">

        </activity>


        <!--Farms-->
        <activity
            android:name=".Farms.FarmsMain"
            android:label="My Farms">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--Processes-->
        <activity android:name=".Processes.ProcessesMain">

        </activity>


        <!--Details-->
        <activity android:name=".DetailProcess">

        </activity>
    </application>

</manifest>